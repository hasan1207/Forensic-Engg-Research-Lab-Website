    <!-- Services Section -->
    <section id="services" class="py-5 bg-dark text-white">
        <div class="container">
            <div class="text-center mb-5">
                <h2 class="display-4 fw-bold mb-3">Our Services</h2>
                <div class="divider-light mx-auto"></div>
                <p class="lead mt-3">Comprehensive forensic solutions for various sectors</p>
            </div>
            <div class="row g-4 services-cards">
                
                <div class="card service-card">
                    <img src="/images/forensic analytical.jpg" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h4>Forensic Analytical</h4>
                        <p class="card-text">Physical Science Discipline (Fingerprints, Questioned Documents, Handwritings & Signatures, Soil,
                            Forged Documents, Lottery Ticket, Counterfeit Currency, Copyright/Trademark Goods, Foot/ Tyre
                            Impressions), Digital & Cyber Forensics, Forensic Accounting & Auditing Investigations (FAI) </p>
                    </div>
                    </div>
                <div class="card service-card">
                    <img src="/images/forensic structural.jpg" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h4>Forensic Structural Engineering</h4>
                        <p class="card-text">Material Testing of Construction Materials (Cement, Concrete, Steel, Bitumen, Aggregate,
                        Masonary, Soil, etc.), Non-Destructive Testing of Structural Members, Concrete Mix Design</p>
                    </div>
                    </div>

                <div class="card service-card">
                    <img src="/images/forensic investigation.jpg" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h4>Forensic Investigation</h4>
                        <p class="card-text">Investigation of Insurance Claims, Crime Scene Forensics, Incident Reconstruction</p>
                    </div>
                    </div>

                    <div class="card service-card">
                    <img src="/images/forensic consultancy.jpg" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h4>Forensic Consultancy</h4>
                        <p class="card-text">Expert Testimony, Legal Consultancy, Medico-legal Consultation, Dispute Resolution Advisory</p>
                    </div>
                    </div>

                    <div class="card service-card">
                    <img src="/images/forensic education.jpg" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h4>Forensic Education & Training</h4>
                        <p class="card-text">Education, Training & Workshops, Academic Initiatives, Research Initiatives, Academic Institutions</p>
                    </div>
                    </div>
                
